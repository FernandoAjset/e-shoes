{
  "AppUrl": "${AppUrl}",
  "SMTP": {
    "HOST": "${HOST}",
    "PORT": ${PORT},
    "USERNAME": "${USERNAME}",
    "PASSWORD": "${PASSWORD}"
  },
  "ConnectionStrings": {
    "ConnectionLCDE": "Server=${DB_SERVER};Database=${DB_DATABASE};User Id=${DB_USER};Password=${DB_PASSWORD};Encrypt=False;TrustServerCertificate=False;",
    "AzureStorage": "DefaultEndpointsProtocol=https;AccountName=${AZ_STORE_ACC_NAME};AccountKey=${AZ_STORE_ACC_KEY};EndpointSuffix=core.windows.net"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
